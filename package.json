{
  "name": "fastify-slonik",
  "description": "Fastify Slonik Plugin",
  "version": "1.0.0",
  "author": "spa5k <unbuttun@gmail.com>",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "fastify": "^3.23.1",
    "fastify-plugin": "^3.0.0",
    "slonik": "^25.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@types/node": "16.11.11",
    "@types/tap": "15.0.5",
    "dotenv": "10.0.0",
    "eslint": "8.2.0",
    "eslint-config-galex": "3.3.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "prettier": "2.4.1",
    "semantic-release": "18.0.0",
    "tap": "15.0.10",
    "tslib": "2.3.1",
    "tsup": "5.7.1",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/spa5k/fastify-slonik",
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "fastify",
    "fastify-plugin",
    "pg",
    "postgres",
    "slonik"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "peerDependencies": {
    "fastify": "3.24.1",
    "slonik": "25.0.0"
  },
  "repository": {
    "url": "https://github.com/spa5k/fastify-slonik",
    "type": "git"
  },
  "scripts": {
    "build": "NODE_ENV='production' tsup-node",
    "build:dev": "NODE_ENV='development' tsup-node",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts",
    "test": "pnpm build && tap -j1 -R classic dist/test/index.test.js"
  },
  "tap": {
    "check-coverage": false
  },
  "typings": "dist/index.d.ts",
  "sideEffects": false
}
