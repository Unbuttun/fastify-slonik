{
  "name": "fastify-slonik",
  "description": "Fastify Slonik Plugin",
  "version": "0.3.4",
  "author": "Unbuttun <unbuttun@gmail.com>",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "fastify": "^3.20.1",
    "fastify-plugin": "^3.0.0",
    "slonik": "^23.8.5"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/node": "16.4.8",
    "@types/tap": "15.0.5",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-galex": "2.16.7",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "np": "7.5.0",
    "prettier": "2.3.2",
    "tap": "15.0.9",
    "ts-node": "10.1.0",
    "tslib": "2.3.0",
    "tsup": "4.13.1",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/Unbuttun/fastify-slonik",
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "fastify",
    "fastify-plugin",
    "pg",
    "postgres",
    "slonik"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "peerDependencies": {
    "fastify": "3.20.1",
    "slonik": "23.9.0"
  },
  "repository": {
    "url": "https://github.com/Unbuttun/fastify-slonik",
    "type": "git"
  },
  "scripts": {
    "build": "tsup-node",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts",
    "test": "pnpm build && tap -j1 -R classic dist/test/index.test.js"
  },
  "tap": {
    "check-coverage": false
  },
  "typings": "dist/index.d.ts"
}
