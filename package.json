{
  "name": "fastify-slonik",
  "description": "Fastify Slonik Plugin",
  "version": "0.3.5",
  "author": "Unbuttun <unbuttun@gmail.com>",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "fastify": "^3.20.1",
    "fastify-plugin": "^3.0.0",
    "slonik": "^24.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/git": "9.0.1",
    "@semantic-release/github": "8.0.1",
    "@semantic-release/npm": "8.0.0",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@types/node": "16.4.13",
    "@types/tap": "15.0.5",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-galex": "2.16.12",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.2",
    "prettier": "2.4.1",
    "semantic-release": "18.0.0",
    "tap": "15.0.10",
    "tslib": "2.3.1",
    "tsup": "5.2.0",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/Unbuttun/fastify-slonik",
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "fastify",
    "fastify-plugin",
    "pg",
    "postgres",
    "slonik"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "peerDependencies": {
    "fastify": "3.21.6",
    "slonik": "24.1.0"
  },
  "repository": {
    "url": "https://github.com/Unbuttun/fastify-slonik",
    "type": "git"
  },
  "scripts": {
    "build": "NODE_ENV='production' tsup-node",
    "build:dev": "NODE_ENV='development' tsup-node",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts",
    "test": "pnpm build && tap -j1 -R classic dist/test/index.test.js"
  },
  "tap": {
    "check-coverage": false
  },
  "typings": "dist/index.d.ts"
}
